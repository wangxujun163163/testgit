function onDOMReady(){initSlideImage()}function initSlideImage(){var a=1e4,b=2e3,c=0,d=$("#slideBox"),e=d.children("li").length,f=0;if(0!=e){for(var g=$("#slideTrigger"),h="",i=0;e>i;i++)h+='<div class="slide_trigger_item"><span class="item_background"></span><span class="item_progress"></span><span class="item_index">'+(i+1)+"</span></div>";g.append($(h)),$("#slideTrigger div.slide_trigger_item").click(function(){h=$(this);var a=h.index();h.hasClass("selected")||(f=a,j())});var j=function(){window.clearTimeout(c),$(d.children("li")).hide(),$(d.children("li")[f]).fadeIn(b),$("#slideTrigger div.slide_trigger_item").removeClass("selected");var h=g.children("div.slide_trigger_item")[f];$(h).addClass("selected"),f++,f>=e&&(f=0),e>1&&(c=window.setTimeout(j,a))};j()}}$(document).ready(onDOMReady);