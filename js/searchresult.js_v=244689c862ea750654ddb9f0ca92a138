function onDOMReady(){initPriceDragBar(),initFilterItem(),initPagination(),initTuanBanner()}function initPriceDragBar(){0!=$("#js-mrco-price-drag-bar-wrap").length&&$("#js-mrco-price-drag-bar-wrap").dragBar({minPrice:price.from,maxPrice:price.to,startPrice:price.pf,endPrice:price.pt,rulerSize:7,changeCallback:function(a){var b=Number(a.startPrice);isNaN(b)&&(b=0);var c=Number(a.endPrice);isNaN(c)&&(c="*");var d=price.base.replace("{{pf}}",b).replace("{{pt}}",c);sendStat("click","is_search_price"),window.location.href=d+"#nav"},priceScale:{"price-0":{index:0,price:0,scale:0,count:1},"price-5":{index:1,price:5,scale:1,count:4},"price-10":{index:2,price:10,scale:1,count:4},"price-15":{index:3,price:15,scale:1,count:4},"price-20":{index:4,price:20,scale:1,count:4},"price-25":{index:5,price:25,scale:1,count:4},"price-30":{index:6,price:30,scale:1,count:4},"price-35":{index:7,price:35,scale:1,count:4},"price-40":{index:8,price:40,scale:5,count:3},"price-60":{index:9,price:60,scale:5,count:3},"price-80":{index:10,price:80,scale:5,count:3},"price-100":{index:11,price:100,scale:0,count:0}}})}function initFilterItem(){var a=$(".filter_wrap"),b=$(a).offset().top||$(a).position().top;b=426,$(window).scroll(function(c){var d=$(window).scrollTop();d>b?$(a).addClass("fixed"):$(a).removeClass("fixed")})}function initPagination(){0!=$("#jumpbtn").length&&$("#jumpbtn").on("click",onPaginationJumpBtnClick)}function onPaginationJumpBtnClick(a){var b=parseInt($.trim($("#jumpnum").val()));if(b>0&&b<=pagination.pages&&b!=pagination.pagth){var c=pagination.base.replace("{{p}}",b);window.location.href=c+"#content"}}function initTuanBanner(){var a=$(".tuan0725_banner_cimg");a.length&&a.on("click",function(a){$(".tuan0725_banner_wrap").remove()})}$(document).ready(onDOMReady);